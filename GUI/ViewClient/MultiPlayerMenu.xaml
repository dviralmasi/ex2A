<Window x:Class="GUI.ViewClient.MultiPlayerMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI.ViewClient"
        mc:Ignorable="d"
        Title="MultiPlayerMenu" Height="300" Width="300">
    <Grid RenderTransformOrigin="1.274,0.483">
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="117,33,0,0" VerticalAlignment="Top" Width="120"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="10,33,0,0" TextWrapping="Wrap" Text="List of games:" VerticalAlignment="Top" Width="130"/>
        <Button x:Name="button" Content="Join Game" HorizontalAlignment="Left" Margin="117,81,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <local:DataUserControl HorizontalAlignment="Left" Margin="10,125,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
